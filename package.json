	"description": "Сборка Gulp - HTML версия",
	"main": "index.js",
	"tipe": "module",
	"scripts": {
		"(1) START - Project (Build)": "gulp",
		"(2) END - Project (Build)": "gulp --production",
		"> CLEAR - (Build)": "gulp clear",
		"> SCSS  - files": "gulp scss",
		"> HTML  - files": "gulp html",
		" >> JavaScript": "gulp js",
		"> IMAGE - files": "gulp image",
		"> FONT  - files": "gulp fonts",
		"> FONT  - style": "gulp fontsStyle",
		"(3) FINAL - Project (Public)": "gulp final",
		"> CLEAR - (Public)": "gulp clearEnd",
		"> CLEAR - (Fonts)": "gulp clearFonts",
		"> CLEAR - (change)": "gulp change",
		"TASKS": "gulp --tasks"
	},
